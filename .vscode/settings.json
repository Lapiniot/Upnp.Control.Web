{
    "files.exclude": {
        ".vs": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/.gitignore": true,
        "**/*.DotSettings.*": true,
        "**/*.DotSettings": true,
        "**/bin/": true,
        "**/obj/": true,
        "**/package-lock.json": true,
        "**/node_modules": true,
        "**/*.user": true,
        "*/**/.vscode": true,
        "**/*.sln": true,
        "**/.gitmodules": true,
        "IoT.Device/IoT.Device.Generators": true,
        "IoT.Device/IoT.Device.Lumi": true,
        "IoT.Device/IoT.Device.Yeelight": true,
        "IoT.Device/IoT.Protocol/IoT.Protocol.Lumi": true,
        "IoT.Device/IoT.Protocol/IoT.Protocol.Yeelight": true,
        "IoT.Device/IoT.Protocol/System.Common/System.Common.CommandLine": true,
        "IoT.Device/IoT.Protocol/System.Common/System.Common.Configuration": true,
        "IoT.Device/IoT.Protocol/System.Common/System.Common.Tests": true,
        "**/react-app-env.d.ts": true,
        "**/.eslintcache": true,
        "Upnp.Control.Web/ClientApp/src/svg/material-design-icons/*": true,
        "Upnp.Control.Web/ClientApp/tsconfig.node.tsbuildinfo": true,
        "Upnp.Control.Web/ClientApp/vite.config.js": true
    },
    "typescript.tsdk": "Upnp.Control.Web/Upnp.Control.Web/ClientApp/node_modules/typescript/lib",
    "auto-rename-tag.activationOnLanguage": [
        "xml",
        "html"
    ],
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "html"
    ],
    "omnisharp.enableRoslynAnalyzers": true,
    "css.customData": [
        "Upnp.Control.Web/ClientApp/css-data.json"
    ],
    "build": {
        "framework": "net7.0",
        "configuration": "Debug"
    },
    "publish": {
        "configuration": "Release"
    },
    "dotnet.defaultSolution": "Upnp.Control.Web.sln"
}